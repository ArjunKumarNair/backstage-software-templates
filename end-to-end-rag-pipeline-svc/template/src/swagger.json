{
    "swagger": "2.0",
    "info": {
        "title": "End-to-End RAG Pipeline API",
        "version": "1.0.0"
    },
    "paths": {
        "/data-prep": {
            "post": {
                "summary": "Data Preparation",
                "parameters": [
                    {
                        "in": "body",
                        "name": "body",
                        "description": "Text to split into chunks",
                        "required": true,
                        "schema": {
                            "type": "object",
                            "properties": {
                                "text": {
                                    "type": "string"
                                }
                            },
                            "required": [
                                "text"
                            ]
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Chunks of text",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "chunks": {
                                    "type": "array",
                                    "items": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/embedChunks": {
            "post": {
                "summary": "Embed Chunks",
                "parameters": [
                    {
                        "in": "body",
                        "name": "body",
                        "description": "Chunks to embed",
                        "required": true,
                        "schema": {
                            "type": "object",
                            "properties": {
                                "chunks": {
                                    "type": "array",
                                    "items": {
                                        "type": "string"
                                    }
                                }
                            },
                            "required": [
                                "chunks"
                            ]
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Embeddings of chunks",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "embeddings": {
                                    "type": "array",
                                    "items": {
                                        "type": "array",
                                        "items": {
                                            "type": "number"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/embedQuery": {
            "post": {
                "summary": "Embed Query",
                "parameters": [
                    {
                        "in": "body",
                        "name": "body",
                        "description": "Query to embed",
                        "required": true,
                        "schema": {
                            "type": "object",
                            "properties": {
                                "query": {
                                    "type": "string"
                                }
                            },
                            "required": [
                                "query"
                            ]
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Embedding of query",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "embedding": {
                                    "type": "array",
                                    "items": {
                                        "type": "number"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/upsert": {
            "post": {
                "summary": "Upsert Documents",
                "parameters": [
                    {
                        "in": "body",
                        "name": "body",
                        "description": "Documents to upsert",
                        "required": true,
                        "schema": {
                            "type": "object",
                            "properties": {
                                "docs": {
                                    "type": "array",
                                    "items": {
                                        "type": "object"
                                    }
                                }
                            },
                            "required": [
                                "docs"
                            ]
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Upsert result",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "message": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/query": {
            "post": {
                "summary": "Query Similar Chunks",
                "parameters": [
                    {
                        "in": "body",
                        "name": "body",
                        "description": "Query string",
                        "required": true,
                        "schema": {
                            "type": "object",
                            "properties": {
                                "query": {
                                    "type": "string"
                                }
                            },
                            "required": [
                                "query"
                            ]
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Similar chunks of text",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "textList": {
                                    "type": "array",
                                    "items": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/llm": {
            "post": {
                "summary": "LLM Answer Generation",
                "parameters": [
                    {
                        "in": "body",
                        "name": "body",
                        "description": "Chunks and question for LLM",
                        "required": true,
                        "schema": {
                            "type": "object",
                            "properties": {
                                "chunks": {
                                    "type": "array",
                                    "items": {
                                        "type": "string"
                                    }
                                },
                                "question": {
                                    "type": "string"
                                }
                            },
                            "required": [
                                "chunks",
                                "question"
                            ]
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "LLM answer",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "answer": {
                                    "type": "string"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Invalid input"
                    }
                }
            }
        },
        "/health": {
            "get": {
                "summary": "Health Check",
                "responses": {
                    "200": {
                        "description": "Service status",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "status": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}